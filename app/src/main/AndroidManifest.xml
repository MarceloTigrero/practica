<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pstlab8">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PSTlab8">
        <activity
            android:name=".BienvenidoActivity"
            android:exported="true" />
        <activity
            android:name=".AcercaDeActivity"
            android:exported="true" />
        <activity
            android:name=".RevisarCorreoActivity"
            android:exported="true" />
        <activity
            android:name=".InicioSeccionActivity"
            android:exported="true" />
        <activity
            android:name=".WebViewActivity"
            android:exported="true" />
        <activity android:name=".InicioActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>

</manifest>